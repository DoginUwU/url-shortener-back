{
  "name": "backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect=4321 --exit-child --transpile-only --ignore-watch node_modules src/shared/infra/server.ts",
    "lint": "eslint src/**/*.ts",
    "build": "tsc-silent --suppress @node_modules/.prisma/ --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "postinstall": "prisma generate && prisma migrate deploy",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --files src/shared/infra/server.ts",
    "docker:up": "docker-compose up -d",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@prisma/client": "^4.2.1",
    "bcrypt": "^5.0.1",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "tsc-silent": "^1.2.2",
    "tscpaths": "^0.0.9",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.13",
    "@types/request-ip": "^0.0.37",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  }
}
